    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "framer-motion": "^2.1.4",
    "http-errors": "^1.8.0",
    "http-status-codes": "^1.4.0",
    "morgan": "^1.10.0",
    "pug": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "server": "node ./bin/www"
  },
